class Solution {
public:
    int majorityElement(vector<int>& nums) {
        // int n=nums.size()/2;
        
        // for(int i=0;i<nums.size();i++){
        //     int c=0;
        //     for(int j=0;j<nums.size();j++){
        //         if(nums[i]==nums[j]){
        //             c++;
        //         }
        //     }
        //     if(c>n)
        //         return nums[i];
        // }
        // return -1;
        ////////////////////////////
    //    int c=0;
    //     int ans; 
    //     for(auto x:nums){
    //         if(c==0)  {ans=x; c++;}
    //         else if(x==ans) c++;
    //         else c--;
    //     }
    //    return ans;
    ////////////////////////////
    unordered_map<int,int> mp;
    
    int n=nums.size();
    for(int i:nums){
        mp[i]++;
        //if(mp[i]>=n/2 && mp[i]>mp[max]) max=i;
        if(mp[i]>n/2) return i;

    }
    return -1;
    }
};
