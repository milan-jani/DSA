class Solution {
public:
    bool uniqueOccurrences(vector<int>& arr) {
        unordered_map<int,int> ans;
        for(int x:arr) ans[x]++;
        unordered_set<int> s;
        for (pair<int,int> p : ans) {
            if (s.find(p.second) != s.end()) {
                return false;
            }
            s.insert(p.second);
        }
        return true;

    }
};
