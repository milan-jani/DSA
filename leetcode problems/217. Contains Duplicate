class Solution {
public:
    bool containsDuplicate(vector<int>& nums) {
    //    sort(nums.begin(), nums.end());
    //     for (int i = 1; i < nums.size(); i++) {
    //         if (nums[i] == nums[i - 1]) {
    //             return true;
    //         }
    //     }
    //     return false;
    // unordered_map<int,int> ans;
    // for(int x:nums){
    //     if(ans[x]>0) return true;
    //     ans[x]++;
    // }
    // return false;
     unordered_set<int> ans;
    for(int x:nums){
        if(ans.count(x)) return true;
        ans.insert(x);
    }
    return false;

    }
};
