class Solution {
public:
    int pal(int n){
        int rev = 0;
        while (n > 0) {
        int digit = n % 10;
        rev = rev * 10 + digit;
        n /= 10;
    }
    return rev;
    }
    int countDistinctIntegers(vector<int>& nums) {
        int c=0;
        unordered_set<int> s1(nums.begin(),nums.end());
        for(int x:nums){
           s1.insert(pal(x));
        }
        return s1.size();
        
    }
};
