class Solution {
  public:
    int rotationCount(long long R, long long D) {
        // code here
        string s1=to_string(R);
        string s2=to_string(D);
        int sum=0;
        for(int i=0;i<s1.length();i++){
            int x=s1[i]-'0';
            int y=s2[i]-'0';
        if(x==y) continue;
        else {
            int diff=abs(x-y);
            sum+= min(diff,10-diff);
        }
        }
        return sum;
        
        
    }
};
