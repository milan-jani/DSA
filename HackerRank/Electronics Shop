int getMoneySpent(vector<int> keyboards, vector<int> drives, int b) {
    int maxCost = -1;

    for (int i = 0; i < keyboards.size(); i++) {
        for (int j = 0; j < drives.size(); j++) {
            int sum = keyboards[i] + drives[j];

            if (sum <= b && sum > maxCost) {
                maxCost = sum;
            }
        }
    }

    return maxCost;
}
