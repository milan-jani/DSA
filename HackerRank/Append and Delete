string appendAndDelete(string s, string t, int k) {
    int common = 0;
    int minLen = min(s.length(), t.length());
    
    for(int i = 0; i < minLen; i++) {
        if(s[i] == t[i]) common++;
        else break;
    }
    
    int ops = (s.length() - common) + (t.length() - common);
    
    if(ops > k) return "No";
    else if((k - ops) % 2 == 0) return "Yes";
    else if(k >= s.length() + t.length()) return "Yes";
    else return "No";
}
